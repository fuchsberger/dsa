#, elixir-format
#: lib/dsa/data/skill.ex:158 lib/dsa/data/skill.ex:406 lib/dsa/data/skill.ex:521
msgid "Combat Maneuvers"
msgstr "Kampfmanöver"

#, elixir-format
#: lib/dsa/data/skill.ex:159 lib/dsa/data/skill.ex:407
msgid "Long Distances"
msgstr "Langstrecke"

#, elixir-format
#: lib/dsa/data/skill.ex:166 lib/dsa/data/skill.ex:414
msgid "Negotiate the distance faster."
msgstr "Die Distanz bis zum Ziel kann schneller überwunden werden."

#, elixir-format
#: lib/dsa/data/skill.ex:160 lib/dsa/data/skill.ex:408
msgid "Races"
msgstr "Wettfahren"

#, elixir-format
#: lib/dsa/data/skill.ex:153
