#: lib/dsa/data/skill.ex:165
msgid "Ship or boat"
msgstr "Schiff oder Boot"

#, elixir-format
#: lib/dsa/data/skill.ex:167
msgid "The hero barely makes any progress with the boat or ship."
msgstr "Der Held kommt mit dem Boot oder Schiff kaum voran."

#, elixir-format
#: lib/dsa/data/skill.ex:169
msgid "The hero falls overboard or damages an important part of the vessel."
msgstr "Der Held fällt von Bord oder ein wichtiger Teil des Wasserfahrzeugs wurde beschädigt."

#, elixir-format
#: lib/dsa/data/skill.ex:168
msgid "The hero uses currents and winds to sail twice as fast as expected."
msgstr "Der Held nutzt günstige Strömungen und Winde, um doppelt so schnell voranzukommen wie üblich."

#, elixir-format
#: lib/dsa/data/skill.ex:209
msgid "Betting Games"
msgstr "Wettspiele"

#, elixir-format
#: lib/dsa/data/skill.ex:210
msgid "Board Games"
msgstr "Brettspiele"

#, elixir-format
#: lib/dsa/data/skill.ex:211
