<%= render "header.html", conn: @conn %>
<h2><%= gettext("Keine Route gefunden fÃ¼r") <> " #{@conn.method} /#{Enum.join(@conn.path_info, "/")}" %></h2>
<%= case @reason do
  %Phoenix.Router.NoRouteError{} -> nil
  %{message: nil} -> nil
  %{message: message} ->
    render DsaWeb.LayoutView, "alert.html", conn: @conn, type: :error, message: message
  _ -> nil
end %>
<%= render "footer.html", conn: @conn %>
