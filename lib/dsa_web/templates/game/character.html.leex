<%= f = form_for @changeset, "#", [
  novalidate: true,
  phx_change: :validate,
  phx_submit: :update
] %>
  <div class='row mb-3'>
    <div class='col-sm'>
      <%= render "card_generel.html", f: f, changeset: @changeset %>
      <%= render "card_skills.html", f: f, changeset: @changeset %>
    </div>
    <div class='col-sm'>
      <%= render "card_traits.html", f: f, changeset: @changeset %>
    </div>
  </div>
</form>
